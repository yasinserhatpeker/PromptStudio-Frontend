{
  "manifest_version": 3,
  "name": "PromptStudio",
  "version": "1.0.0",
  "description": "Manage and organize your AI prompts",
  "action": {
    "default_popup": "index.html",
    "default_title": "PromptStudio"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
